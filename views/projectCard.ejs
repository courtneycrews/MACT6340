<div class="card m-2 h-100 fadein">
    <img src="<%= image %>" class="card-img-top" alt="An image of the <%= name %> project">
    <div>
        <div class="card-title">
            <h3>
                <%= name %>
            </h3>
        </div>
    </div> 
    <div class="card-footer">
        <a href="/artwork/<%= ident %>" class="btn custom-button">View Artwork</a>
    </div>
</div>